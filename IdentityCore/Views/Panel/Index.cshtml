@model AppUser
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover table-sm">
    <tr>
        <th>İsim:</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Soyisim:</th>
        <td>@Model.SurName</td>
    </tr>
    <tr>
        <th>Email:</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Telefon:</th>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <th>Resim:</th>
        <td>@Model.PictureUrl</td>
    </tr>
</table>
